{
    "root": true,
    "env": {
        "es2021": true
    },
    "plugins": [
        "eslint-plugin-prettier",
        "prettier",
        "sonarjs"
    ],
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:sonarjs/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "no-plusplus": "off",
        "no-console": "off",
        "no-unused-vars": "warn",
        "prefer-template":"off",
        "sonarjs/prefer-immediate-return":"off"
    }
}